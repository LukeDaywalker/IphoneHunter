{
  "manifest_version": 2,
  "name": "IphoneHunter",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
//    "page": "background.html"
  },
  "browser_action": {
    "default_title": "click me",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "background",
    "notifications",
    "contextMenus"
  ]
}  