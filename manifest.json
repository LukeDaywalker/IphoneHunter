{
  "manifest_version": 2,
  "name": "IphoneHunter",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "click me",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://*.apple.com/*",
    "*://netpay.cmbchina.com/*",
    "webRequest",
    "webRequestBlocking",
    "background",
    "notifications",
    "contextMenus"
  ]
}  